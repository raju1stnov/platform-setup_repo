{
   "candidates_db_main": {
     "sink_id": "candidates_db_main",
     "name": "HR Candidates DB",
     "description": "Main database for candidate records generated by HRRecruitingAssistant.",
     "sink_type": "sqlite",
     "connection_ref": "/app/data/candidates.db",
     "schema_definition": {
        "table_name": "candidates",
        "columns": [
           {"name": "id", "type": "INTEGER", "pk": true},
           {"name": "name", "type": "TEXT"},
           {"name": "title", "type": "TEXT"},
           {"name": "skills", "type": "TEXT"}
        ]
     },
     "query_agent_method": "dbservice_agent.execute_query",
     "schema_agent_method": "dbservice_agent.get_schema"
   },
   "error_logs_main": {
    "sink_id": "error_logs_main",
    "name": "System Error Logs (BigQuery)",
    "description": "Cloud error logs routed to BigQuery from Pub/Sub.",
    "sink_type": "bigquery",
    "connection_ref": "bigquery://low-code-poc-kdas-2025.low_code_dataset.log_table",
    "schema_definition": {
      "table_name": "log_table",
      "columns": [
        { "name": "log_entry", "type": "JSON" }
      ]
    },
    "query_agent_method": "log_router_agent.execute_query",
    "schema_agent_method": "log_router_agent.get_schema"
  }
}